{
  "name": "editor-pro",
  "version": "0.2.0",
  "private": true,
  "description": "A feature rich Text and Code Editor that combines document sharing, editing, and compilation capabilities",
  "engines": {
    "node": "20.10.0",
    "npm": "10.4.0"
  },
  "main": "index.js",
  "scripts": {
    "dev": "clear && nodemon start",
    "start": "node index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "cd client && npm run build && cd .. && rm -rf view && mkdir view && mv client/build/* view"
  },
  "keywords": [
    "code",
    "code-editor",
    "compiler",
    "live-sharing",
    "web-sockets"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/HarshRajGupta/editor-pro.git"
  },
  "bugs": {
    "url": "https://github.com/HarshRajGupta/editor-pro/issues"
  },
  "homepage": "https://editor-pro.onrender.com",
  "author": "HarshRajGupta",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.9",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5"
  }
}
